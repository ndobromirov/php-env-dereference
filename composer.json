{
    "name": "ndobromirov/php-env-dereference",
    "description": "PHP environment variables dereference library.",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Dobromirov",
            "email": "ndobromirov@gmail.com"
        }
    ],
    "autoload":{
        "psr-4":{
            "EnvDereference\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "EnvDereference\\": "tests/"
        }
    },
    "require": {
        "php": ">=5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "scripts": {
        "cs": "phpcs --standard=PSR2 -n src tests",
        "test": "phpunit"
    },
     "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    }
}
